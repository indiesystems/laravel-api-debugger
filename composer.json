{
    "name": "indiesystems/laravel-api-debugger",
    "description": "A Laravel package for debugging API requests/responses with session-based logging and AdminLTE UI",
    "keywords": ["laravel", "api", "debugger", "logging", "requests", "responses", "adminlte", "bootstrap", "multi-tenant"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Chris Dimas",
        "email": "chris@indie.systems",
        "homepage": "https://indie.systems",
        "role": "Developer"
    }],
    "homepage": "https://github.com/indiesystems/laravel-api-debugger",
    "support": {
        "issues": "https://github.com/indiesystems/laravel-api-debugger/issues",
        "source": "https://github.com/indiesystems/laravel-api-debugger"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IndieSystems\\ApiDebugger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IndieSystems\\ApiDebugger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IndieSystems\\ApiDebugger\\Providers\\ApiDebuggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
